{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/menu",
            "destination": "/menu.html"
        },
        {
            "source": "/venue",
            "destination": "/venue.html"
        },
        {
            "source": "/story",
            "destination": "/story.html"
        },
        {
            "source": "/gallery",
            "destination": "/gallery.html"
        },
        {
            "source": "/private-hire",
            "destination": "/private-hire.html"
        },
        {
            "source": "/contact",
            "destination": "/contact.html"
        },
        {
            "source": "/faqs",
            "destination": "/faqs.html"
        }
    ],
    "trailingSlash": false
}